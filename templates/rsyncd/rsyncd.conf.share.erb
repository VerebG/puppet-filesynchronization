
[<%=@name%>]
    path = <%=@path%>
    log file = <%=@logfile%>
    comment = <%=@comment%>
    read only = <%=@readonly%>
    write only = <%=@writeonly%>
    list = no
    uid = <%=@uid%>
    gid = <%=@gid%>
    incoming chmod = <%=@incomechmod%>
    outgoing chmod = <%=@outgochmod%>
    hosts allow = <%=@hostsallow%>